<h1>Users</h1>
<% @users.each do |user| %>
  <%= link_to user do %>
    <%= image_tag user.user_image.url %>
  <% end %>

  <div class="full-name">
    <%= user.full_name %>
  </div>

  <div class="email">
    <%= user.email %>
  </div>

  <div class="username">
    <%= user.username %>
  </div>

  <%= user.created_at.strftime("%d %b. %Y") %>

<% end %>
