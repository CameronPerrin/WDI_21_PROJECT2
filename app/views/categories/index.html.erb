<p id="notice"><%= notice %></p>

<h1>Categories</h1>
<div class="row">
  <% @categories.each do |category| %>
    <div class="medium-3 columns dummy">
    <%= link_to category do %>
      <div class="panel categories">
        <%= category.name %>
      </div>
    <% end %>
    </div>
  <% end %>
</div>

